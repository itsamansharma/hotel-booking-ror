
<h1>Property listing here</h1>
<h2>only active properties</h2>

<div id="properties">
	<div>
		<%= button_to 'sign out',destroy_user_session_path, method: :delete %>	
	</div>
	<br>
	<div>
		<%= button_to 'my bookings', my_bookings_path	, method: :get , message:'this user has' %>	
	</div>

	<br>
	
	<table>
		<thead>
			<tr>
				<th>property name</th>
				<th> address</th>
				<th> price</th>
				<th> pin code</th>
			</tr>
		</thead>
		<% @property.each do |p| %>
		<tbody>
			<tr>
				<td><%= p.property_name %></td>
				<td><%= p.address %></td>
				<td><%= p.price %></td>
				<td><%= p.pin %></td>
				<td><%= link_to "Book Now", new_booking_path(property_id: p.id) %></td>
			</tr>
		</tbody>
		<%end%>
	</table>
		
</div>